        {
          "expr": "sum(increase(claude_code_cost_usage_USD_total{user_name=~\"$user\",project_name=~\"$project\"}[$__range]))",
          "legendFormat": "cost",
          "refId": "A"
        }
      ]
    },
    {
      "datasource": { "type": "prometheus", "uid": "victoriametrics" },
